{
  "name": "task-manager",
  "version": "1.0.0",
  "description": "web app project that allows a user to create and manipulate different tasks.",
  "default": "src/index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run static && npx parcel src/index.html",
    "static": "npx gulp",
    "dev": "npm run static && npx parcel src/index.html",
    "build": "npm run static && npx parcel build src/index.html",
    "clean:dev": "rm -rf .parcel-cache dist",
    "clean:git": "rm -rf .parcel-cache dist  node_modules"
  },
  "keywords": [
    "task",
    "task-manager",
    "todo",
    "todo-list"
  ],
  "author": "Valeriya Karetnikova",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/ValeriyaKaretnikova/task-manager"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.0.0-rc.0",
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "@parcel/transformer-sass": "^2.0.0-rc.0",
    "@parcel/transformer-webmanifest": "^2.0.0-rc.0",
    "gulp": "^4.0.2",
    "http-server": "^13.0.2",
    "parcel": "^2.0.0-rc.0",
    "parcel-plugin-json-url-loader": "^0.1.2",
    "sass": "^1.40.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "uuid": "^8.3.2"
  }
}
